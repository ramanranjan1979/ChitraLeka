@model IEnumerable<ChitraLeka.ViewModel.Contact.Contact>


<div class="panel panel-default">
    <div class="panel-heading">
        <strong>Contact Number List</strong>
    </div>

    @if (Model.Count() > 0)
    {
       
        
         <table class="table">
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.ContactNumberType)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Value)
                </th>                
            </tr>

            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.ContactNumberType)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Value)
                    </td>
                   
                </tr>
            }
        </table>
    }
    else
    {
        @: <div class="text-muted">&nbsp;No contact number.</div>
    }
</div>



